package com.mkpits.datatypes;

public class CharacterDatatype {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       char wantingToEatPizza = 'y';
       char wantingToEatburger = 'N';
       
       System.out.println("Pizza :"+wantingToEatPizza);
       System.out.println("burger :"+wantingToEatburger);
	}

}
//char has 2 byte space
//char default value is/u0000-no output
//char datatype is also called textual literalsssssssssssssss